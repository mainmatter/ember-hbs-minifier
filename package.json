{
  "name": "ember-hbs-minifier",
  "version": "0.4.0",
  "description": "Stripping whitespace out of your Handlebars templates",
  "keywords": [
    "ember-addon"
  ],
  "homepage": "https://github.com/simplabs/ember-hbs-minifier",
  "repository": "github:simplabs/ember-hbs-minifier",
  "license": "MIT",
  "author": "simplabs GmbH <info@simplabs.com>",
  "files": [
    "hbs-minifier-plugin.js",
    "index.js",
    "utils"
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "changelog": "lerna-changelog",
    "lint": "eslint . --cache",
    "start": "ember server",
    "test": "jest",
    "test:ember": "ember test"
  },
  "jest": {
